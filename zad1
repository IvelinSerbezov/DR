package zadachi;
import java.util.Scanner;
public class zad1dr {
	public static void main(String[] args) {
		// TODO Auto-generated method stub
Scanner inp = new Scanner(System.in);
int n=inp.nextInt();
int sumc=0;
int sumn=0;
for(int i=0;i<n;i++) {
	int C=inp.nextInt();
	if(C%2==0)sumc=C+sumc;
	else sumn=C+sumn;
}
if(sumc==sumn)System.out.println("Yes "+sumc);
else System.out.println("No "+(sumc-sumn));
	}
}
